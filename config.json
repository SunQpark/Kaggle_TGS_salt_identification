{
    "name": "Unet_Res34encoder",
    "cuda": true,
    "gpu": 0,
    "data_loader": {
        "data_dir": "./input/",
        "batch_size": 256,
        "shuffle": true     
    },
    "validation": {
        "validation_split": 0.05,
        "shuffle": true,
        "fold": 0
    },
 
    "optimizer_type": "Adam",
    "optimizer": {
        "lr": 0.002,
        "weight_decay": 0,
        "amsgrad": true
    },
    "loss": "bce_with_l1_loss",
    "metrics": ["mean_iou"],
    "trainer": {
        "epochs": 500,
        "save_dir": "saved/",
        "save_freq": 1,
        "verbosity": 2,
        "monitor": "val_mean_iou",
        "monitor_mode": "max"
    },
    "arch": "ResnetUnet", 
    "model": {
        "n_fts": 32
    }
}
